        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CartAndProfile">
        <activity
            android:name=".OrderConfirm"
            android:exported="true" />
        <activity
            android:name=".SendEmail"
            android:exported="true" />
        <activity
            android:name=".RVActivity"
            android:exported="true" />
        <activity
            android:name=".EnterProfile"
            android:exported="true" />
        <activity
            android:name=".ShowCart"
            android:exported="true" />
        <activity
            android:name=".Cart"
            android:exported="true" />
        <activity
            android:name=".Showprofile"
            android:exported="true" />
        <activity
            android:name=".CreateProfile"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>